{
    "name": "origoenergia/azure-sdk-php-fork-servicebus",
    "description": "This is a fork of the PHP Azure SDK for the service bus ",
    "keywords": ["origoenergia", "php", "azure", "sdk" ],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Kaique Prazeres",
            "email": "contact@kaiqueprazeres.dev"
        }
    ],
    "require": {
        "php": ">=7.2.0",
        "pear/net_url2": "^2.2",
        "pear/mail_mime": "^1.10",
        "guzzlehttp/guzzle": "^7.0",
        "laminas/laminas-mime": "^2.6",
        "laminas/laminas-mail": "^2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "phpdocumentor/phpdocumentor": "^2.8"
    },
    "autoload": {
        "psr-4": { "AzureServiceBus\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" }
    }
}
